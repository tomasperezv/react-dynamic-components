{
  "name": "react-dynamic-components",
  "version": "0.0.9",
  "description": "React Dynamic Components",
  "scripts": {
    "test": "jest",
    "build": "babel src -d lib --ignore src/js/vendor,src/js/__tests__ && browserify --standalone Textarea lib/js/*.js > lib/js/react-components.js"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/babel-jest",
    "preprocessCachingDisabled": true,
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/fbjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasperezv/react-textarea.git"
  },
  "keywords": [
    "react",
    "jest"
  ],
  "author": "Tomas Perez <t@0x101.com>",
  "license": "MIT",
  "homepage": "https://github.com/tomasperezv/react-dynamic-components#readme",
  "devDependencies": {
    "babel-jest": "^9.0.3",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "jest-cli": "^0.9.2",
    "react": "^0.14.7",
    "react-addons": "^0.9.1-deprecated",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "webpack": "^1.12.14"
  }
}
